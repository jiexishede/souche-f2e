#header
    script.
        function flushUserTag(){
            $.ajax({  
                type:"get",
                dataType:"jsonp",
                url: "http://115.29.10.121:9988/loginAction/getCookie.json",
                crossDomain:true,
                jsonpCallback:"setUserInfo",
                success: function(data){
                    flushUserTagIframe(data);
                },
                beforeSend:function(){
                }, 
                complete:function(data,status){
                }  
            });
        }
        function flushUserTagIframe(data){
            var url = 'http://127.0.0.1:8080/flush_user_tag.htm' + '?userTag=' + data.userTag + "&userId=" + data.userId;
            $("#flushUserTagIframe").attr("src", url);
        }
        $("#flushUserTagIframeSouche").attr("src", "http://115.29.10.121:9988/loginAction/getCookie.json");
        $(document).ready(function(){
            setTimeout("flushUserTag()", 2000);
        });
