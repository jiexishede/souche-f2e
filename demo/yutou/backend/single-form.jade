  .control-group
    label.control-label 总库存数:
    .controls
      input#carSum.input-small(type='text', value='', name='carSum', placeholder='具体库存数量，单位：台')
      span#carSum_err.help-inline(style='color: #B94A48;')
  .control-group
    label.control-label 开店日期:
    .controls
      .input-append.date
        input#openDate.m-wrap.date-picker(type='text', value='', name='openDate', placeholder='yyyy-mm-dd')
        span.add-on
          i.icon-calendar
      span#openDate_err.help-inline(style='color: #B94A48;')
