form#form.form-horizontal(name='form', method='post', enctype='multipart/form-data')
  input(type='hidden', name='token', value='true')
  h3.form-section 公司基本信息
  .control-group
    label.control-label 公司名称:
    .controls
      input#name.input-large(type='text', name='name', value='', placeholder='公司全称，详细，如北京大搜车汽车服务西苑北店')
      span#name_err.help-inline(style='color: #B94A48;')
  .control-group
    label.control-label 公司简称:
    .controls
      input#nickname.input-large(type='text', name='nickname', value='', placeholder='简称，如：北京大搜车')
      span#nickname_err.help-inline(style='color: #B94A48;')
  .control-group
    label.control-label 固定电话:
    .controls
      input#addresscall.input-large(type='text', value='', name='addresscall', placeholder='固定电话。如：0577-65899785')
      span#addresscall_err.help-inline(style='color: #B94A48;')
  .control-group
    label.control-label 移动电话:
    .controls
      input#phone.input-large(type='text', value='', name='phone', placeholder='手机号码,如：18869987598')
      span#phone_err.help-inline(style='color: #B94A48;')
  .control-group
    label.control-label 地址:
    .controls
      input#place.input-large.input-block-level(type='text', value='', name='place', placeholder='公司详细地址，精确到门牌号')
      span#place_err.help-inline(style='color: #B94A48;') 错误信息
  .control-group
    label.control-label 省份城市:
    .controls
      select#province.input-medium(style='width: 80px;', name='province', tabindex='1')
        option(value='') -不限-
        option(value='00001') 北京
        option(value='03095') 新疆
        option(value='00530') 辽宁
        option(value='01840') 湖南
        option(value='02914') 甘肃
        option(value='00105') 河北
        option(value='00419') 内蒙古
        option(value='00043') 天津
        option(value='02245') 海南
        option(value='01193') 福建
        option(value='02799') 陕西
        option(value='00974') 浙江
        option(value='03231') 台湾
        option(value='02272') 四川
        option(value='02718') 西藏
        option(value='02474') 贵州
        option(value='02572') 云南
        option(value='00021') 上海
        option(value='00289') 山西
        option(value='01288') 江西
        option(value='02122') 广西
        option(value='00714') 黑龙江
        option(value='03066') 宁夏
        option(value='00858') 江苏
        option(value='01075') 安徽
